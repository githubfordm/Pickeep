<?xml version="1.0" encoding="UTF-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="vi.filepicker.EnterActivity">

    <ImageView
        android:id="@+id/deskImage"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:scaleType="fitXY"
        android:src="@drawable/tree_modify" />

    <Button
        android:id="@+id/imageButton"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginBottom="15dp"
        android:layout_marginEnd="17dp"
        android:layout_marginRight="17dp"
        android:background="@drawable/main_plus"
        android:scaleType="fitStart" />
    //책을 나타낸다.

    <ImageButton
        android:id="@+id/one"
        android:layout_width="60dp"
        android:layout_height="80dp"
        android:layout_marginLeft="34dp"
        android:layout_marginStart="34dp"
        android:layout_marginTop="20dp"
        android:background="@android:color/transparent"
        android:onClick="showContents"
        android:scaleType="fitXY"
        android:src="@drawable/book_2"
        android:visibility="invisible" />

    <TextView
        android:id="@+id/textOne"
        android:background="@android:color/white"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/one"
        android:layout_alignRight="@+id/one"
        android:visibility="invisible"/>

    <ImageButton
        android:id="@+id/two"
        android:layout_width="60dp"
        android:layout_height="80dp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="20dp"
        android:background="@android:color/transparent"
        android:onClick="showContents"
        android:scaleType="fitXY"
        android:src="@drawable/book_2"
        android:visibility="invisible" />

    <TextView
        android:id="@+id/textTwo"
        android:background="@android:color/white"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/two"
        android:layout_alignRight="@+id/two"
        android:visibility="invisible"/>

    <ImageButton
        android:id="@+id/three"
        android:layout_width="60dp"
        android:layout_height="80dp"
        android:layout_marginEnd="18dp"
        android:layout_marginRight="18dp"
        android:background="@android:color/transparent"
        android:onClick="showContents"
        android:scaleType="fitXY"
        android:src="@drawable/book_2"
        android:visibility="invisible"
        android:layout_alignTop="@+id/two"
        android:layout_alignRight="@+id/imageButton"
        android:layout_alignEnd="@+id/imageButton" />

    <TextView
        android:id="@+id/textThree"
        android:background="@android:color/white"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/three"
        android:layout_alignRight="@+id/three"
        android:visibility="invisible"/>

    <ImageButton
        android:id="@+id/four"
        android:layout_width="60dp"
        android:layout_height="80dp"
        android:layout_alignLeft="@+id/one"
        android:layout_alignStart="@+id/one"
        android:layout_below="@+id/one"
        android:layout_marginTop="25dp"
        android:background="@android:color/transparent"
        android:onClick="showContents"
        android:scaleType="fitXY"
        android:src="@drawable/book_2"
        android:visibility="invisible" />

    <TextView
        android:id="@+id/textFour"
        android:background="@android:color/white"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/four"
        android:layout_alignRight="@+id/four"
        android:visibility="invisible"/>

    <ImageButton
        android:id="@+id/five"
        android:layout_width="60dp"
        android:layout_height="80dp"
        android:layout_alignLeft="@+id/two"
        android:layout_alignStart="@+id/two"
        android:layout_alignTop="@+id/four"
        android:background="@android:color/transparent"
        android:onClick="showContents"
        android:scaleType="fitXY"
        android:src="@drawable/book_2"
        android:visibility="invisible" />

    <TextView
        android:id="@+id/textFive"
        android:background="@android:color/white"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/five"
        android:layout_alignRight="@+id/five"
        android:visibility="invisible"/>

    <ImageButton
        android:id="@+id/six"
        android:layout_width="60dp"
        android:layout_height="80dp"
        android:layout_alignLeft="@+id/three"
        android:layout_alignStart="@+id/three"
        android:layout_alignTop="@+id/four"
        android:background="@android:color/transparent"
        android:onClick="showContents"
        android:scaleType="fitXY"
        android:src="@drawable/book_2"
        android:visibility="invisible" />

    <TextView
        android:id="@+id/textSix"
        android:background="@android:color/white"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/six"
        android:layout_alignRight="@+id/six"
        android:visibility="invisible"/>

    <ImageButton
        android:id="@+id/seven"
        android:layout_width="60dp"
        android:layout_height="80dp"
        android:background="@android:color/transparent"
        android:onClick="showContents"
        android:scaleType="fitXY"
        android:src="@drawable/book_2"
        android:visibility="invisible"
        android:layout_alignTop="@+id/eight"
        android:layout_alignLeft="@+id/four"
        android:layout_alignStart="@+id/four" />

    <TextView
        android:id="@+id/textSeven"
        android:background="@android:color/white"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/seven"
        android:layout_alignRight="@+id/seven"
        android:visibility="invisible"/>

    <ImageButton
        android:id="@+id/eight"
        android:layout_width="60dp"
        android:layout_height="80dp"
        android:layout_marginTop="35dp"
        android:background="@android:color/transparent"
        android:scaleType="fitXY"
        android:src="@drawable/book_2"
        android:visibility="invisible"
        android:layout_below="@+id/five"
        android:layout_alignLeft="@+id/five"
        android:layout_alignStart="@+id/five" />

    <TextView
        android:id="@+id/textEight"
        android:background="@android:color/white"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/eight"
        android:layout_alignRight="@+id/eight"
        android:visibility="invisible"/>

    <ImageButton
        android:id="@+id/nine"
        android:layout_width="60dp"
        android:layout_height="80dp"
        android:layout_alignLeft="@+id/six"
        android:layout_alignStart="@+id/six"
        android:layout_alignTop="@+id/eight"
        android:background="@android:color/transparent"
        android:onClick="showContents"
        android:scaleType="fitXY"
        android:src="@drawable/book_2"
        android:visibility="invisible" />

    <TextView
        android:id="@+id/textNine"
        android:background="@android:color/white"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/nine"
        android:layout_alignRight="@+id/nine"
        android:visibility="invisible"/>

    <ImageButton
        android:id="@+id/ten"
        android:layout_width="60dp"
        android:layout_height="80dp"
        android:background="@android:color/transparent"
        android:onClick="showContents"
        android:scaleType="fitXY"
        android:src="@drawable/book_2"
        android:visibility="invisible"
        android:layout_below="@+id/seven"
        android:layout_alignLeft="@+id/textSeven"
        android:layout_alignStart="@+id/textSeven" />

    <TextView
        android:id="@+id/textTen"
        android:background="@android:color/white"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/ten"
        android:layout_alignRight="@+id/ten"
        android:visibility="invisible"/>

    <ImageButton
        android:id="@+id/eleven"
        android:layout_width="60dp"
        android:layout_height="80dp"
        android:layout_alignLeft="@+id/eight"
        android:layout_alignStart="@+id/eight"
        android:layout_alignTop="@+id/ten"
        android:background="@android:color/transparent"
        android:scaleType="fitXY"
        android:src="@drawable/book_2"
        android:visibility="invisible" />

    <TextView
        android:id="@+id/textEleven"
        android:background="@android:color/white"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/eleven"
        android:layout_alignRight="@+id/eleven"
        android:visibility="invisible"/>

    <ImageButton
        android:id="@+id/twelve"
        android:layout_width="60dp"
        android:layout_height="80dp"
        android:layout_alignLeft="@+id/nine"
        android:layout_alignStart="@+id/nine"
        android:layout_alignTop="@+id/eleven"
        android:background="@android:color/transparent"
        android:onClick="showContents"
        android:scaleType="fitXY"
        android:src="@drawable/book_2"
        android:visibility="invisible" />

    <TextView
        android:id="@+id/textTwelve"
        android:background="@android:color/white"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/twelve"
        android:layout_alignRight="@+id/twelve"
        android:visibility="invisible"/>

    <ImageButton
        android:id="@+id/thirteen"
        android:layout_width="60dp"
        android:layout_height="80dp"
        android:layout_marginBottom="13dp"
        android:background="@android:color/transparent"
        android:elevation="0dp"
        android:onClick="showContents"
        android:scaleType="fitXY"
        android:src="@drawable/book_2"
        android:visibility="invisible"
        android:layout_alignBottom="@+id/imageButton"
        android:layout_alignLeft="@+id/ten"
        android:layout_alignStart="@+id/ten" />

    <TextView
        android:id="@+id/textThirteen"
        android:background="@android:color/white"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/thirteen"
        android:layout_alignRight="@+id/thirteen"
        android:visibility="invisible"/>

    <ImageButton
        android:id="@+id/fourteen"
        android:layout_width="60dp"
        android:layout_height="80dp"
        android:background="@android:color/transparent"
        android:onClick="showContents"
        android:scaleType="fitXY"
        android:src="@drawable/book_2"
        android:visibility="invisible"
        android:layout_below="@+id/ten"
        android:layout_alignLeft="@+id/eleven"
        android:layout_alignStart="@+id/eleven" />

    <TextView
        android:id="@+id/textFourteen"
        android:background="@android:color/white"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/fourteen"
        android:layout_alignRight="@+id/fourteen"
        android:visibility="invisible"/>

</RelativeLayout>